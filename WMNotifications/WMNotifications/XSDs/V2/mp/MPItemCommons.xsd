  </xsd:simpleType>

  <xsd:complexType name="WeightMeasure">
    <xsd:sequence>
      <xsd:element name="value" type="xsd:double" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
            Please provide the value for the weight.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="unit" type="UnitOfWeight" minOccurs="1">
      	 <xsd:annotation>
          <xsd:documentation>
            Please provide the unit of measure for the weight.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="UnitOfWeight">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="KG"/>
      <xsd:enumeration value="G"/>
      <xsd:enumeration value="MG"/>
      <xsd:enumeration value="LB"/>
      <xsd:enumeration value="OZ"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="TimeMeasure">
    <xsd:sequence>
      <xsd:element name="value" type="xsd:double" minOccurs="1">
      </xsd:element>
      <xsd:element name="unit" type="UnitOfTime" minOccurs="1">
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="UnitOfTime">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="S" />
      <xsd:enumeration value="MIN" />
      <xsd:enumeration value="HR" />
      <xsd:enumeration value="DAY" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="Money">
    <xsd:sequence>
      <xsd:element name="currency" type="CurrencyCode" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
            Represents  the Currency of the price 
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="amount" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
            Represents  the price 
          </xsd:documentation>
        </xsd:annotation>       
        <xsd:simpleType>
          <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="11"/>
            <xsd:fractionDigits value="4"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  
